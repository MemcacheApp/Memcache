{
    "name": "Memcache Saver",
    "description": "Memcache Saver",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "index.html"
    },
    "permissions": ["cookies", "contextMenus", "tabs"],
    "host_permissions": ["*://localhost/*"],
    "background": {
        "scripts": ["background.js"]
    }
}
